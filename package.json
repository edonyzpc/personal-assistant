    "builtin-modules": "3.3.0",
    "esbuild": "0.17.19",
    "esbuild-plugin-copy": "2.1.1",
    "eslint": "8.40.0",
    "jest": "^29.5.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.0",
